START

    ; TODO Step #5 - Insert Your Program Here
BANKSEL PORTA
    CLRF PORTA
    BANKSEL PORTC
    CLRF PORTC
   MOVLW 0x01
   MOVWF PORTC
   BANKSEL ANSEL
   CLRF ANSEL
   BANKSEL TRISC
   CLRF TRISC
   BANKSEL PORTA
   
   EST
   BTFSC PORTA,0
   GOTO SUM
   BTFSC PORTA,1
   GOTO REST
   BTFSC PORTA,2
   GOTO OFF
   GOTO EST
   
   SUM
   INCFSZ PORTC 
   GOTO EST
   
   REST
   DECFSZ PORTC
   GOTO EST
   
   OFF
   MOVLW 0x01
   MOVWF PORTC
   GOTO EST
   
   
   END
